/* RESET
=============================================================================*/

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
}

/* BODY
=============================================================================*/


/*.all { padding:0; width:100%; margin:0 auto; }*/

/*body { background:#f7f7f7; /*font-family:Helvetica, STHeiti, Droid Sans Fallback;*/ /*-webkit-text-size-adjust:100%; -ms-text-size-adjust:100%; -webkit-tap-highlight-color:transparent; }*/


ul, ol { padding-left:1.2rem; }


.you_have { text-align:center; border-bottom:.1rem solid rgba(0,0,0,0.1); padding:0 0 2rem; margin:0 0 2rem; }
.you_have span { display:block; }
.you_have span { color:rgba(248,78,148,1); font-size:1.6rem; margin:.6rem 0; }
.you_have .img { background-image:url(../images/ww.png); width:8rem; height:8rem; background-size:100% 100%; margin:0 auto; margin-top:3rem; margin-bottom:.4rem; }


/*********** item ***********/
.item_single { margin:1rem 0; }

.item { position:relative; width:100%; background:#fff; border-top:.1rem solid rgba(0,0,0,.1); border-bottom:.1rem solid rgba(0,0,0,.1); padding:1.4rem 0; }
.item .title { position:absolute; left:1rem; color:rgba(0,0,0,0.8); font-size:1.3rem; }
.item .input { width:100%; }
.item .input input { display:inline-block; text-align:right; width:98%; border:none; color:rgba(0,0,0,.4); font-size:1rem; -webkit-appearance:none;
box-shadow:none; -webkit-box-shadow:none; outline:none; font-size:1.3rem; font-family:inherit; }

.item_append { width:96%; margin:0 auto 2rem; text-align:left; font-size:1.3rem; color:rgba(0,0,0,.6); }
.item_append span { color:rgba(255,96,153,.9); }
.item_attach { border-top:none; margin-bottom:1rem; }
.item_attach2 { border-top:none; margin-bottom:0; }
.item_head { border-top:none; margin-bottom:0; border-top-left-radius:.4rem; border-top-right-radius:.4rem; }
.item_bottom { border-bottom:none; margin-bottom:0; border-bottom-left-radius:.4rem; border-bottom-right-radius:.4rem; }

.explanation { margin-top:1rem; font-size:1.4rem;  color:rgba(0,0,0,0.7); margin-bottom:2rem; padding:1rem 2rem; }
.explanation span { color:rgba(248,78,148,1); display:inline-block; }
.explanation p { padding:.2rem 0; color:rgba(0,0,0,1); }
.explanation li { line-height:1.3; margin:.6rem 0; vertical-align:middle; }
.explanation li .icon_qq { display:inline-block; width:1.3rem; height:1.3rem; margin:0 0 0 .6rem; vertical-align:top; background:url(../images/connect_qq.png) no-repeat; background-size:100% 100%; }
/************************/
.the_btn { width:100%; margin:1rem auto; text-align:center; }
.btn_exchange,
.btn_disable { width:96%; margin:0 auto; display:inline-block; text-align:center; background-color:rgba(255,96,153,.9); color:rgba(255,255,255,1); text-align:center; text-decoration:none; padding:1.2rem 0; border-radius:.5rem; font-size:1.4rem; }
.btn_exchange:active { background-color:rgba(255,96,153,1); color:rgba(255,255,255,.9); }

a.btn_disable { background-color:rgba(248,78,148,.4); color:rgba(255,255,255,.6); }
a.btn_disable:link { background-color:rgba(248,78,148,.4); color:rgba(255,255,255,.6); }
a.btn_disable:visited { background-color:rgba(248,78,148,.4); color:rgba(255,255,255,.6); }
a.btn_disable:hover { background-color:rgba(248,78,148,.4); color:rgba(255,255,255,.6); }
a.btn_disable:active { background-color:rgba(248,78,148,.4); color:rgba(255,255,255,.6); }



.icon_success { background-image:url(../images/success.png); width:6rem; height:6rem; background-size:100% 100%; margin:0 auto; margin-top:4rem; }
.text_ok { font-size:2.4rem; text-align:center; line-height:2; border-top:.1rem solid rgba(0,0,0,.1); margin-top:2rem; }
.text_detail { text-align:center; color:rgba(0,0,0,.5); font-size:1.2rem; }



/*******card_money*********/
.card_money { border:.15rem solid #e6a03c; width:94%; margin:1rem auto 1rem; border-radius:.7rem; padding:1.4rem 0 1rem; text-align:center; color:rgba(255,255,255,1);
background: -webkit-linear-gradient(left, #ffb955 , #e6a03c); /* Safari 5.1 - 6.0 */
background: -moz-linear-gradient(right, #ffb955 , #e6a03c); /* Firefox 3.6 - 15 */
background: linear-gradient(to right, #ffb955 , #e6a03c); /* 标准的语法 */ }


.card_money .currency { text-align:center; color:rgba(255,255,255,.8); font-size:1.2rem; }
.card_money .count { padding:1.4rem 0 1rem; }
.card_money .count .icon_coin { display:inline-block; margin:0 .2rem; width:3rem; height:3rem; background:url(../images/coin.png) no-repeat; background-size:100% 100%; vertical-align:middle; }
.card_money .count .text { display:inline-block; color:#fffeb4; font-size:2.6rem; vertical-align:middle; text-shadow:.1rem .1rem .1rem rgba(0,0,0,0.1); }

.card_money .tips { padding:.4rem 0 .4rem; }
.card_money .tips .icon_tips { display:inline-block; width:1.2rem; margin:0 .3rem; height:1.2rem; background:url(../images/warning.png) no-repeat; background-size:100% 100%; vertical-align:middle; }
.card_money .tips .text { display:inline-block; color:rgba(255,255,255,.4); font-size:1.2rem; vertical-align:middle; }

.card_money .btn { width:90%; margin:.6rem auto .4rem; }
.card_money .btn a { display:block; border:.2rem solid rgba(255,255,255,1); padding:1rem 0; font-size:1.3rem; color:rgba(255,255,255,1); text-decoration:none; border-radius:.5rem; }


/******************************/

.card_baby { border:.15rem solid #f56c9d; width:94%; margin:1rem auto 1rem; border-radius:.7rem; padding:1.4rem 0 1rem; text-align:center; color:rgba(255,255,255,1);
background: -webkit-linear-gradient(left, #ff8eb7 , #ff76a8); /* Safari 5.1 - 6.0 */
background: -moz-linear-gradient(right, #ff8eb7 , #ff76a8); /* Firefox 3.6 - 15 */
background: linear-gradient(to right, #ff8eb7 , #ff76a8); /* 标准的语法 */ }


.card_baby .currency { text-align:center; color:rgba(255,255,255,1); font-size:1.2rem; padding-top:1rem; padding-bottom:.4rem; }
.card_baby .count { position:relative; padding:1.4rem 0 1rem; background:rgba(250,124,171,.9); border-radius:.4rem; width:60%; margin:1rem auto; }
.card_baby .count .arrow { position:absolute; top:-2rem; left:40%; transform:rotate(-90deg); -webkit-transform:rotate(-90deg); border:1rem solid transparent; /*border-left:2rem solid rgba(250,124,171,.9);*/ border-left-color:rgba(250,124,171,.9); width:0; height:0; }
.card_baby .count .icon_baby { display:inline-block; margin:0 .2rem; width:4rem; height:4rem; background:url(../images/ww.png) no-repeat; background-size:100% 100%; vertical-align:middle; }
.card_baby .count .text { display:inline-block; color:#fff; font-size:2.6rem; vertical-align:middle; text-shadow:.1rem .1rem .1rem rgba(0,0,0,0.1); }

.card_baby .tips { padding:.4rem 0 .4rem; }
.card_baby .tips .icon_tips { display:inline-block; width:1rem; margin:0 .3rem; height:1rem; background:url(../images/warning.png) no-repeat; background-size:100% 100%; vertical-align:middle; }
.card_baby .tips .text { display:inline-block; color:rgba(255,255,255,.4); font-size:1.2rem; vertical-align:middle; padding:1rem 0; }

.card_baby .btn { width:90%; margin:.6rem auto .4rem; }
.card_baby .btn a { display:inline-block; margin:0 1rem; border:.2rem solid rgba(255,255,255,1); padding:1rem 1.4rem; font-size:1.0rem; color:rgba(255,255,255,1); text-decoration:none; border-radius:.5rem; }







.one_log { display:block; border:.1rem solid rgba(0,0,0,.1); width:90%; margin:1.4rem auto; border-radius:.4rem; }
.one_log .item { padding:.6rem 0; }
.one_log .item .input input { width:94%; }
.one_log .item .input span { display:inline-block; text-align:right; width:96%; border:none; color:rgba(0,0,0,.4); font-size:1rem; -webkit-appearance:none;
box-shadow:none; -webkit-box-shadow:none; outline:none; font-size:1.3rem; font-family:inherit; }














