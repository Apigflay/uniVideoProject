.miao_rank .item2 { height:13rem; }
.miao_rank .item2 .icon2 { position:absolute; left:1rem; top:44%; margin:0; }
.miao_rank .item2 .the_img { width:12rem; height:12rem; margin:0 auto; display:block; }
.miao_rank .item2 .the_img .img { display:inline-block; position:absolute; z-index:2; width:12rem; height:12rem; margin:0 auto; background-image:url(http://liveimg.9158.com/pic/week/img_bg2.png); background-size:100% 100%; }
.miao_rank .item2 .the_img .img_img { position:absolute; z-index:1; width:5rem; height:5rem; margin:0 auto; top:2.6rem; left:0; bottom:0; right:0;  }
