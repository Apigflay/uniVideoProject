.the_level .bg6 { background-image:url(images/level240_3x.png); }
.the_level .num { z-index:2; color:#fff; left:1.15rem; bottom:.1rem; font-family:'num'; font-size:1rem; width:2.168rem; text-align:center; letter-spacing:-.2rem; -webkit-transform:scale(0.68);   }

