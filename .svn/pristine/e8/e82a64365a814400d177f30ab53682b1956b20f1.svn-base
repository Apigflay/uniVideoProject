.miao_rank .item .add,.miao_rank .item .add_s{ float:right; width:2.5rem; height:2.5rem; background:url(../images/icon_user_add.png) no-repeat; background-size:100%; margin-top:1.6rem; margin-right:.9rem;}
.miao_rank .item .add_s{width:2.5rem; height:2.5rem; background:url(../images/icon_user_add_s.png) no-repeat; background-size:100%; }


.tab .on{ color:#f14e94; border-bottom:.2rem solid rgba(241,78,148,1); margin-bottom:-.1rem;}

.miao_rank .rank_text { text-align:center; margin-top:0.9rem;display:none; }
.miao_rank .rank_text span { display:inline-block; color:rgba(255,255,255,.4); font-size:1.1rem; margin-bottom:2rem; }

