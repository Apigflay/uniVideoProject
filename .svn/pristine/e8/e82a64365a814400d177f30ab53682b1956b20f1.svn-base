.miao_rank .item2 .name .food { z-index:3; color:rgba(0,0,0,.4); padding-top:0; }

.miao_rank .item3 { height:12.5rem; }
.miao_rank .item3 .icon3 { position:absolute; left:1rem; top:44%; margin:0; }
.miao_rank .item3 .the_img { width:12rem; height:12rem; margin:0 auto; display:block; }
.miao_rank .item3 .the_img .img { display:inline-block; position:absolute; z-index:2; width:12rem; height:12rem; margin:0 auto; background-image:url(http://liveimg.9158.com/pic/week/img_bg3.png); background-size:100% 100%; }
.miao_rank .item3 .the_img .img_img { position:absolute; z-index:1; width:4.8rem; height:4.8rem; margin:0 auto; top:1.8rem; left:0; bottom:0; right:0;  }
.miao_rank .item3 .the_img .img_img img { width:4.8rem; height:4.8rem; }
.miao_rank .item3 .name { display:inline-block; width:96%; text-align:center; margin-top:-4.2rem; z-index:2; }
.miao_rank .item3 .name .food { z-index:3; color:rgba(0,0,0,.4); padding-top:0; }


.tab { position:fixed; width:100%; margin:0 auto; z-index:9;border-bottom:.1rem solid rgba(0,0,0,.1); background-color:#f8f5f6; }
.tab .tab_item { display:inline-block; width:32%; text-align:center; padding:1rem 0; font-size:1.5rem; }
.tab .tab_sel { color:#f14e94; border-bottom:.2rem solid rgba(241,78,148,1); margin-bottom:0rem; }

.black { position:absolute; top:0; right:0; bottom:0; left:0; z-index:8; background-color:rgba(0,0,0,.6); width:100%; height:100%; }
.miao_rank .position_card { position:absolute; z-index:9; top:0; right:0; bottom:0; left:0; margin:auto; width:82%; height:26rem; border-radius:1.1rem; background-color:#fff; }
.miao_rank .card { position:relative; }
.miao_rank .card .police,
.miao_rank .card .close,
.miao_rank .card .img,
.miao_rank .card .img_img { position:absolute; }
.miao_rank .card .police { background-image:url(images/police2x.png); left:1rem; top:.8rem; width:1.6rem; height:1.6rem; background-size:100% 100%; }
.miao_rank .card .close { background-image:url(images/close_profile2x.png); right:1rem; top:1rem; width:1.2rem; height:1.2rem; background-size:100% 100%; }

.miao_rank .card .img { top:2rem; right:0; bottom:0; left:0; z-index:11; background-image:url(images/profile_img_bg.png); width:8rem; height:8rem; margin:0 auto; background-size:100% 100%; }
.miao_rank .card .img_img { top:2rem; right:0; bottom:0; left:0; z-index:10; width:8rem; height:8rem; margin:0 auto; }
.miao_rank .card .img_img img { width:8rem; height:8rem; margin:0 auto; }

.miao_rank .card .text { background-color:#f5f5f5; text-align:center; margin:0 auto; border-radius:1.1rem; }


